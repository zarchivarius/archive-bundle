{
    "name": "zarchivarius/archive-bundle",
    "description": "Controls archived records",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksandr Zagovorychev",
            "email": "zagovorichev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/config": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/dependency-injection": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Zarchivarius\\ArchiveBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zarchivarius\\ArchiveBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2",
        "api-platform/core": "^2.6",
        "symfony/framework-bundle": "^5.2",
        "symfony/browser-kit": "^5.2",
        "symfony/http-client": "^5.2"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
